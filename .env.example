# OpenAI API Configuration
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Anthropic API Configuration (optional)
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_api_key_here

# Google API Configuration (optional)
GOOGLE_GENAI_USE_VERTEXAI=1
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_CLOUD_LOCATION=us-central1
AGENT_ENGINE_ID=your_agent_engine_id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service_account_key.json
SERVICE_ACCOUNT_EMAIL=your_service_account@your_project.iam.gserviceaccount.com

# Groq API Configuration (optional)
GROQ_API_KEY=gsk_your_groq_api_key_here

# Hugging Face API Configuration (optional)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# DeepTeam Configuration
DEEPTEAM_MODEL_PROVIDER=anthropic  # Options: openai, anthropic, google, azure, huggingface
DEEPTEAM_MODEL_NAME=claude-3-5-sonnet-20241022  # Claude Sonnet for testing
DEEPTEAM_ATTACK_MODEL=claude-3-5-sonnet-20241022  # Claude Sonnet for generating attacks
DEEPTEAM_EVAL_MODEL=llama-3.1-8b-instant # Groq Llama 3.1 for evaluating responses

# Testing Configuration
MAX_WORKERS=5  # Number of parallel workers for testing
TIMEOUT_SECONDS=30  # Timeout for each test
VERBOSE=true  # Enable verbose logging

# CONFIDENT_AI Configuration
CONFIDENT_AI_API_KEY=your_confident_ai_api_key_here

# Tavily Configuration
TAVILY_API_KEY=your_tavily_api_key_here